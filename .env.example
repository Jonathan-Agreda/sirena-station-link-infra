# ============================
# Sirena Station Link - .env.example
# ============================

# --- Postgres (superusuario - controla ambas DBs) ---
POSTGRES_SUPERUSER=postgres
POSTGRES_SUPERPASS=postgres

# --- Postgres (Backend Sirena) ---
POSTGRES_DB=sirena_db
POSTGRES_USER=sirena
POSTGRES_PASSWORD=sirena_pass

# --- Postgres (Keycloak) ---
KC_DB=keycloak_db
KC_DB_USER=keycloak
KC_DB_PASSWORD=keycloak_pass

# --- Keycloak (admin inicial) ---
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin123

# JDBC a Postgres (Keycloak DB aislada)
KC_DB_URL=jdbc:postgresql://postgres:5432/${KC_DB}
KC_DB_USERNAME=${KC_DB_USER}
KC_DB_PASSWORD=${KC_DB_PASSWORD}

# --- EMQX Dashboard ---
EMQX_DASHBOARD_USER=admin
EMQX_DASHBOARD_PASS=admin123

# --- Credenciales backend para MQTT ---
MQTT_BACKEND_USER=srv-backend
MQTT_BACKEND_PASS=srv-backend-secret

# --- Dispositivo semilla (para pruebas MQTT) ---
SEED_DEVICE_ID=SRN-001
SEED_DEVICE_APIKEY=srn-api-key-001

# --- EMQX cookie de cl√∫ster ---
EMQX_NODE_COOKIE=sirena_cookie_supersegura
